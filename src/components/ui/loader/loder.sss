.loader
  animation: spinner 600ms linear infinite
  border-color: rgba(0, 0, 0, 0.4) rgba(0, 0, 0, 0) rgba(0, 0, 0, 0) rgba(0, 0, 0, 0.4)
  border-radius: 10px
  border-style: solid
  boder-width: 2px
  display: block
  width: 15px
  height: 15px
  margin-top: 10px
  margin-bottom: 10px

@keyframes spinner
  from
    transform: rotate(0deg)
  to
    transform: rotate(360deg)



